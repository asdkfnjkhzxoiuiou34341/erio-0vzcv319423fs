        createItemToggle(getText("LuckyStoneMask"), _G.YBAHacksModule and _G.YBAHacksModule.YBAConfig.ItemESP.Items["Lucky Stone Mask"] or true)
        createItemToggle(getText("LuckyArrow"), _G.YBAHacksModule and _G.YBAHacksModule.YBAConfig.ItemESP.Items["Lucky Arrow"] or true)
        
        -- ü§ñ AUTOFARM –∑–∞–≥–æ–ª–æ–≤–æ–∫
        createSectionHeader("ü§ñ AUTOFARM")
        
        -- Autofarm —Ç—É–º–±–ª–µ—Ä
        createToggleSlider("Autofarm", _G.YBAHacksModule and _G.YBAHacksModule.isAutofarmEnabled() or false, function(v)
            if _G.YBAHacksModule then
                if v then
                    _G.YBAHacksModule.startAutofarm()
                else
                    _G.YBAHacksModule.stopAutofarm()
                end
            else
                print("üéØ KAKASHKI: YBA Hacks –º–æ–¥—É–ª—å –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω!")
            end
        end)
        
        -- –ó–∞–≥–æ–ª–æ–≤–æ–∫ –¥–ª—è –≤—ã–±–æ—Ä–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞
        createSectionHeader("üì¶ ITEMS FARM")
        
        -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª–µ–π –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞
        local function createAutofarmItemToggle(itemName, defaultState)
            local btn = createToggleSlider(itemName, defaultState, function(v)
                if _G.YBAHacksModule then
                    _G.YBAHacksModule.AutofarmConfig.Items[itemName] = v
                    
                    -- –§–ò–ö–°: –°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∏—Ä—É–µ–º –û–ë–ê –≤–∞—Ä–∏–∞–Ω—Ç–∞ –∫–ª—é—á–µ–π –¥–ª—è –ø—Ä–µ–¥–º–µ—Ç–æ–≤
                    if itemName == "MysteriousArrow" or itemName == "Mysterious Arrow" or itemName == "–¢–∞–∏–Ω—Å—Ç–≤–µ–Ω–Ω–∞—è —Å—Ç—Ä–µ–ª–∞" then
                        _G.YBAHacksModule.AutofarmConfig.Items["Mysterious Arrow"] = v -- —Å –ø—Ä–æ–±–µ–ª–æ–º (–û–°–ù–û–í–ù–û–ô –∫–ª—é—á –¥–ª—è –ø–æ–∏—Å–∫–∞)
                        _G.YBAHacksModule.AutofarmConfig.Items["MysteriousArrow"] = v -- –±–µ–∑ –ø—Ä–æ–±–µ–ª–∞
                    elseif itemName == "GoldCoin" or itemName == "Gold Coin" or itemName == "–ó–æ–ª–æ—Ç–∞—è –º–æ–Ω–µ—Ç–∞" then
                        _G.YBAHacksModule.AutofarmConfig.Items["Gold Coin"] = v -- —Å –ø—Ä–æ–±–µ–ª–æ–º
                        _G.YBAHacksModule.AutofarmConfig.Items["GoldCoin"] = v -- –±–µ–∑ –ø—Ä–æ–±–µ–ª–∞
                    end
                    
                    print("ü§ñ AUTOFARM: –ü—Ä–µ–¥–º–µ—Ç", itemName, "—É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –≤", v and "ON" or "OFF")
                    
                    -- –ï—Å–ª–∏ –∞–≤—Ç–æ—Ñ–∞—Ä–º –∞–∫—Ç–∏–≤–µ–Ω, –æ–±–Ω–æ–≤–ª—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ (–∑–∞–≥–ª—É—à–∫–∞ –¥–ª—è –ø–æ–ª–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏)
                    if _G.YBAHacksModule.isAutofarmEnabled() then
                        print("ü§ñ AUTOFARM: –û–±–Ω–æ–≤–ª—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞ –¥–ª—è –ø—Ä–µ–¥–º–µ—Ç–∞:", itemName)
                    end
                else
                    print("üéØ KAKASHKI: YBA Hacks –º–æ–¥—É–ª—å –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω!")
                end
            end)
            return btn
        end
        
        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª–∏ –¥–ª—è –≤—Å–µ—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞
        createAutofarmItemToggle("Mysterious Arrow", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Mysterious Arrow"] or true)
        createAutofarmItemToggle("Rokakaka", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Rokakaka"] or true)
        createAutofarmItemToggle("Pure Rokakaka", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Pure Rokakaka"] or true)
        createAutofarmItemToggle("Diamond", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Diamond"] or true)
        createAutofarmItemToggle("Gold Coin", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Gold Coin"] or true)
        createAutofarmItemToggle("Steel Ball", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Steel Ball"] or true)
        createAutofarmItemToggle("Clackers", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Clackers"] or true)
        createAutofarmItemToggle("Caesar's Headband", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Caesar's Headband"] or true)
        createAutofarmItemToggle("Zeppeli's Hat", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Zeppeli's Hat"] or true)
        createAutofarmItemToggle("Zeppeli's Scarf", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Zeppeli's Scarf"] or true)
        createAutofarmItemToggle("Quinton's Glove", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Quinton's Glove"] or false)
        createAutofarmItemToggle("Stone Mask", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Stone Mask"] or true)
        createAutofarmItemToggle("Rib Cage of The Saint's Corpse", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Rib Cage of The Saint's Corpse"] or true)
        createAutofarmItemToggle("Ancient Scroll", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Ancient Scroll"] or true)
        createAutofarmItemToggle("DIO's Diary", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["DIO's Diary"] or true)
        createAutofarmItemToggle("Lucky Stone Mask", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Lucky Stone Mask"] or true)
        createAutofarmItemToggle("Lucky Arrow", _G.YBAHacksModule and _G.YBAHacksModule.AutofarmConfig.Items["Lucky Arrow"] or true)
        
        -- AUTOSELL —Å–µ–∫—Ü–∏—è (–∑–∞–≥—Ä—É–∑–∫–∞ –º–æ–¥—É–ª—è)
        createDivider()
        createSectionHeader("ü§ñ AUTO SELL")
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∑–∞–≥—Ä—É–∂–µ–Ω –ª–∏ –º–æ–¥—É–ª—å –∞–≤—Ç–æ—Å–µ–ª–ª–∞
        if _G.AutosellModule then
            -- –ú–æ–¥—É–ª—å —É–∂–µ –∑–∞–≥—Ä—É–∂–µ–Ω, –∏—Å–ø–æ–ª—å–∑—É–µ–º –µ–≥–æ —Ñ—É–Ω–∫—Ü–∏—é createGUI
            if _G.AutosellModule.createGUI and type(_G.AutosellModule.createGUI) == "function" then
                print("ü§ñ AUTOSELL: –°–æ–∑–¥–∞–µ–º GUI —á–µ—Ä–µ–∑ –º–æ–¥—É–ª—å...")
                print("ü§ñ AUTOSELL: –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –ø–µ—Ä–µ–¥ –≤—ã–∑–æ–≤–æ–º:", {functionsContainer = functionsContainer, currentY = currentY})
                local newCurrentY = _G.AutosellModule.createGUI(functionsContainer, currentY, createToggleSlider, createSlider, createDivider, createSectionHeader, createButton)
                print("ü§ñ AUTOSELL: –ü–æ–ª—É—á–∏–ª–∏ –æ–±—Ä–∞—Ç–Ω–æ currentY =", newCurrentY)
                currentY = newCurrentY or currentY
            else
                -- Fallback –Ω–∞ —Å—Ç–∞—Ä—ã–π —Å–ø–æ—Å–æ–± –µ—Å–ª–∏ —Ñ—É–Ω–∫—Ü–∏—è createGUI –Ω–µ–¥–æ—Å—Ç—É–ø–Ω–∞
                createSectionHeader("ü§ñ AUTO SELL CONTROLS")
                
                -- –ì–ª–∞–≤–Ω—ã–π —Ç—É–º–±–ª–µ—Ä –∞–≤—Ç–æ—Å–µ–ª–ª–∞
                createToggleSlider("Auto Sell Enabled", false, function(v)
                    if _G.AutosellModule then
                        if v then
                            _G.AutosellModule.start()
                        else
                            _G.AutosellModule.stop()
                        end
                    end
                end)
                
                -- –ö–Ω–æ–ø–∫–∏ –±—ã—Å—Ç—Ä–æ–≥–æ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
                createButton("Enable All Items for Sale", function()
                    if _G.AutosellModule and _G.AutosellModule.enableAllItems then
                        _G.AutosellModule.enableAllItems()
                    end
                end)
                
                createButton("Disable All Items for Sale", function()
                    if _G.AutosellModule and _G.AutosellModule.disableAllItems then
                        _G.AutosellModule.disableAllItems()
                    end
                end)
            end
        else
            -- –ú–æ–¥—É–ª—å –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –∑–∞–≥—Ä—É–∑–∫–∏
        end

        -- –ö–Ω–æ–ø–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ –º–æ–¥—É–ª—è –∞–≤—Ç–æ—Å–µ–ª–ª–∞
        local loadButton = createButton("Load Autosell Module", function()
            print("ü§ñ AUTOSELL: –ó–∞–≥—Ä—É–∂–∞–µ–º –º–æ–¥—É–ª—å –∞–≤—Ç–æ—Å–µ–ª–ª–∞...")
            
            if _G.AutosellModule then
                print("ü§ñ AUTOSELL: –ú–æ–¥—É–ª—å —É–∂–µ –∑–∞–≥—Ä—É–∂–µ–Ω! –û–±–Ω–æ–≤–ª—è–µ–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å...")
                if loadButton then loadButton.Visible = destroy end
                showContent("YBA Hacks")
                return
            end
            
            -- –ü—ã—Ç–∞–µ–º—Å—è —Å–Ω–∞—á–∞–ª–∞ –∑–∞–≥—Ä—É–∑–∏—Ç—å –ª–æ–∫–∞–ª—å–Ω—ã–π –º–æ–¥—É–ª—å, –∑–∞—Ç–µ–º HTTP
            local success, result = pcall(function()
                local autosellCode = readfile("ckvb9wuefh9831")
                return loadstring(autosellCode)()
            end)
            
            if not success then
                success, result = pcall(function()
                    local autosellCode = readfile("autosell.lua")
                    return loadstring(autosellCode)()
                end)
            end
            
            if not success then
                success, result = pcall(function()
                    local autosellCode = game:HttpGet("https://raw.githubusercontent.com/asdkfnjkhzxoiuiou34341/erio-0vzcv319423fs/refs/heads/main/pizdec")
                    return loadstring(autosellCode)()
                end)
            end
            
            if success and result then
                print("ü§ñ AUTOSELL: –ú–æ–¥—É–ª—å –∑–∞–≥—Ä—É–∂–µ–Ω, —Å–æ–∑–¥–∞–µ–º GUI...")
                
                -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —Ñ—É–Ω–∫—Ü–∏—é createGUI –∏–∑ –∑–∞–≥—Ä—É–∂–µ–Ω–Ω–æ–≥–æ –º–æ–¥—É–ª—è
                if _G.AutosellModule and _G.AutosellModule.createGUI and type(_G.AutosellModule.createGUI) == "function" then
                    print("ü§ñ AUTOSELL: –°–æ–∑–¥–∞–µ–º GUI —á–µ—Ä–µ–∑ –∑–∞–≥—Ä—É–∂–µ–Ω–Ω—ã–π –º–æ–¥—É–ª—å...")
                    print("ü§ñ AUTOSELL: –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –ø–µ—Ä–µ–¥ –≤—ã–∑–æ–≤–æ–º:", {functionsContainer = functionsContainer, currentY = currentY})
                    local newCurrentY = _G.AutosellModule.createGUI(functionsContainer, currentY, createToggleSlider, createSlider, createDivider, createSectionHeader, createButton)
                    print("ü§ñ AUTOSELL: –ü–æ–ª—É—á–∏–ª–∏ –æ–±—Ä–∞—Ç–Ω–æ currentY =", newCurrentY)
                    currentY = newCurrentY or currentY
                else
                    -- Fallback –Ω–∞ —Å—Ç–∞—Ä—ã–π —Å–ø–æ—Å–æ–± –µ—Å–ª–∏ —Ñ—É–Ω–∫—Ü–∏—è createGUI –Ω–µ–¥–æ—Å—Ç—É–ø–Ω–∞
                    createDivider()
                    createSectionHeader("ü§ñ AUTO SELL CONTROLS")
                    
                    -- –ì–ª–∞–≤–Ω—ã–π —Ç—É–º–±–ª–µ—Ä –∞–≤—Ç–æ—Å–µ–ª–ª–∞
                    createToggleSlider("Auto Sell Enabled", false, function(v)
                        if _G.AutosellModule then
                            if v then
                                _G.AutosellModule.start()
                            else
                                _G.AutosellModule.stop()
                            end
                        end
                    end)
                    
                    -- –ö–Ω–æ–ø–∫–∏ –±—ã—Å—Ç—Ä–æ–≥–æ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
                    createButton("Enable All Items for Sale", function()
                        if _G.AutosellModule and _G.AutosellModule.enableAllItems then
                            _G.AutosellModule.enableAllItems()
                        end
                    end)
                    
                    createButton("Disable All Items for Sale", function()
                        if _G.AutosellModule and _G.AutosellModule.disableAllItems then
                            _G.AutosellModule.disableAllItems()
                        end
                    end)
                end
                
                print("ü§ñ AUTOSELL: GUI —Å–æ–∑–¥–∞–Ω! –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –¥–æ—Å—Ç—É–ø–Ω–æ –≤—ã—à–µ.")
                
                -- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–∞ —á—Ç–æ–±—ã –Ω–æ–≤—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã –æ—Ç–æ–±—Ä–∞–∑–∏–ª–∏—Å—å
                functionsContainer.Size = UDim2.new(1, 0, 0, currentY)
                
                -- –ü—Ä–∏–Ω—É–¥–∏—Ç–µ–ª—å–Ω–æ –æ–±–Ω–æ–≤–ª—è–µ–º –ø—Ä–æ–∫—Ä—É—Ç–∫—É
                if scrollFrame then
                    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, currentY)
                end
                
                -- –°–∫—Ä—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –∑–∞–≥—Ä—É–∑–∫–∏ –ø–æ—Å–ª–µ —É—Å–ø–µ—à–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏
                if loadButton then loadButton = false end
                
                -- –î–µ–ª–∞–µ–º –∑–∞–¥–µ—Ä–∂–∫—É –∏ –ø–æ–ª–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
                task.wait(0.5)
                showContent("YBA Hacks")
            else
                print("ü§ñ AUTOSELL: –û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ –º–æ–¥—É–ª—è:", tostring(result))
                print("ü§ñ AUTOSELL: –ü–æ–ø—Ä–æ–±—É–π—Ç–µ:")
                print("1. –†–∞–∑–º–µ—Å—Ç–∏—Ç—å autosell.lua –æ–Ω–ª–∞–π–Ω –∏ –æ–±–Ω–æ–≤–∏—Ç—å URL")
                print("2. –ò–ª–∏ –ø–æ–º–µ—Å—Ç–∏—Ç—å autosell.lua –≤ –ø–∞–ø–∫—É workspace")
            end
        end)
    elseif tabName == "Settings" then
        contentTitle.Text = getText("MenuSettings")
        

        
        createDivider()
        
        local accentColorLabel = Instance.new("TextLabel", functionsContainer)
        accentColorLabel.Text = getText("AccentColor")
        accentColorLabel.TextColor3 = Color3.new(1,1,1)
        accentColorLabel.BackgroundTransparency = 1
        accentColorLabel.Size = UDim2.new(1, -10, 0, 20)
        accentColorLabel.Position = UDim2.new(0, 5, 0, currentY)
        accentColorLabel.Font = Enum.Font.Gotham
        accentColorLabel.TextSize = 14
        accentColorLabel.TextXAlignment = Enum.TextXAlignment.Left
        currentY = currentY + 20 + padding

        local accentColors = {
            Color3.fromRGB(0, 150, 0),
            Color3.fromRGB(0, 100, 255),
            Color3.fromRGB(255, 0, 100),
            Color3.fromRGB(255, 150, 0),
            Color3.fromRGB(150, 0, 255),
            Color3.fromRGB(255, 255, 0),
            Color3.fromRGB(0, 255, 255),
            Color3.fromRGB(255, 50, 50),
        }

        local colorRow = Instance.new("Frame", functionsContainer)
        colorRow.Size = UDim2.new(1, -10, 0, 35)
        colorRow.Position = UDim2.new(0, 5, 0, currentY)
        colorRow.BackgroundTransparency = 1

        local colorLayout = Instance.new("UIListLayout", colorRow)
        colorLayout.FillDirection = Enum.FillDirection.Horizontal
        colorLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left
        colorLayout.Padding = UDim.new(0, 8)

        for _, color in pairs(accentColors) do
            local colorBtn = Instance.new("TextButton", colorRow)
            colorBtn.Size = UDim2.new(0, 30, 0, 30)
            colorBtn.BackgroundColor3 = color
            colorBtn.Text = ""
            colorBtn.AutoButtonColor = false
            colorBtn.BorderSizePixel = 0
            
            local colorCorner = Instance.new("UICorner", colorBtn)
            colorCorner.CornerRadius = UDim.new(0, 6)
            
            if MenuSettings.AccentColor == color then
                local highlight = Instance.new("UIStroke", colorBtn)
                highlight.Color = Color3.new(1, 1, 1)
                highlight.Thickness = 2
            end
            
            colorBtn.MouseButton1Click:Connect(function()
                MenuSettings.AccentColor = color
                updateAccentColor()
                
                for _, btn in pairs(colorRow:GetChildren()) do
                    if btn:IsA("TextButton") then
                        local stroke = btn:FindFirstChild("UIStroke")
                        if stroke then stroke:Destroy() end
                        
                        if btn.BackgroundColor3 == color then
                            local highlight = Instance.new("UIStroke", btn)
                            highlight.Color = Color3.new(1, 1, 1)
                            highlight.Thickness = 2
                        end
                    end
                end
                
                showContent(selectedTab)
            end)
        end
        
        currentY = currentY + 35 + padding
        
        createDivider()
        
        createDropdown(getText("Language"), {"English", "Russian"}, MenuSettings.Language, function(selectedLanguage)
            MenuSettings.Language = selectedLanguage
            updateAllTexts()
        end)
    end
    
    functionsContainer.Size = UDim2.new(1, 0, 0, currentY)
    
    selectedTab = tabName
    
    if scrollFrame and tabScrollPositions[tabName] then
        scrollFrame.CanvasPosition = Vector2.new(0, tabScrollPositions[tabName])
    end
end

showContent("Main")

UserInputService.InputBegan:Connect(function(input, gp)
    if not gp and input.KeyCode == Enum.KeyCode.Insert then
        mainFrame.Visible = not mainFrame.Visible
    end
end)
