-- ==================== –ê–í–¢–û–ú–ê–¢–ò–ß–ï–°–ö–ê–Ø –ó–ê–ì–†–£–ó–ö–ê YBA HACKS MODULE ====================

print("üéØ KAKASHKI: –ù–∞—á–∏–Ω–∞–µ–º –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫—É—é –∑–∞–≥—Ä—É–∑–∫—É YBA Hacks –º–æ–¥—É–ª—è...")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–≥—Ä—É–∑–∫–∏ YBA Hacks –º–æ–¥—É–ª—è
local function loadYBAHacksModule()
    local success, result = pcall(function()
        print("üéØ KAKASHKI: –ü–æ–ø—ã—Ç–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ yba_hacks.lua...")
        local ybaHacksCode = readfile("yba_hacks.lua")
        return loadstring(ybaHacksCode)()
    end)
    
    if success then
        print("üéØ KAKASHKI: YBA Hacks –º–æ–¥—É–ª—å —É—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω!")
        return true
    else
        print("üéØ KAKASHKI: –û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ YBA Hacks –º–æ–¥—É–ª—è:", tostring(result))
        print("üéØ KAKASHKI: –£–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ —Ñ–∞–π–ª yba_hacks.lua –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ —Ç–æ–π –∂–µ –ø–∞–ø–∫–µ")
        return false
    end
end

-- –ó–∞–≥—Ä—É–∂–∞–µ–º –º–æ–¥—É–ª—å YBA Hacks –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏ —Å—Ç–∞—Ä—Ç–µ
task.spawn(function()
    task.wait(0.5) -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ –æ—Å–Ω–æ–≤–Ω—ã—Ö –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–æ–≤
    
    if loadYBAHacksModule() then
        print("üéØ KAKASHKI: YBA Hacks –º–æ–¥—É–ª—å –≥–æ—Ç–æ–≤ –∫ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—é!")
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –º–æ–¥—É–ª—å –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ —ç–∫—Å–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–ª—Å—è
        if _G.YBAHacksModule then
            print("üéØ KAKASHKI: –ü—Ä–æ–≤–µ—Ä–∫–∞ —ç–∫—Å–ø–æ—Ä—Ç–∞ –º–æ–¥—É–ª—è - –û–ö")
            print("üéØ KAKASHKI: –î–æ—Å—Ç—É–ø–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ YBA Hacks:")
            print("  - startYBA/stopYBA (Stand Range)")
            print("  - startUndergroundControl/stopUndergroundControl")
            print("  - startAntiTimeStop/stopAntiTimeStop")
            print("  - startItemESP/stopItemESP")
            print("  - startAutofarm/stopAutofarm")
        else
            print("üéØ KAKASHKI: –û–®–ò–ë–ö–ê - –ú–æ–¥—É–ª—å –Ω–µ —ç–∫—Å–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–ª—Å—è –≤ _G.YBAHacksModule")
        end
    else
        print("üéØ KAKASHKI: YBA Hacks –º–æ–¥—É–ª—å –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω, —Ñ—É–Ω–∫—Ü–∏–∏ –±—É–¥—É—Ç –Ω–µ–¥–æ—Å—Ç—É–ø–Ω—ã")
    end
end)

print("üéØ KAKASHKI: –ê–≤—Ç–æ–∑–∞–≥—Ä—É–∑–∫–∞ YBA Hacks –Ω–∞—Å—Ç—Ä–æ–µ–Ω–∞")

