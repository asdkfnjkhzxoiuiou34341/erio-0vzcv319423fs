        antiTimeStopBtn.BackgroundColor3 = Color3.fromRGB(255,100,100)
        antiTimeStopBtn.AutoButtonColor = false
        Instance.new("UICorner", antiTimeStopBtn).CornerRadius = UDim.new(0,6)
