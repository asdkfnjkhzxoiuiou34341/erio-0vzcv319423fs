        antiTimeStopBtn.Size = UDim2.new(1, -10, 0, 28)
        antiTimeStopBtn.Position = UDim2.new(0, 5, 0, currentY)
        antiTimeStopBtn.Text = getText("AntiTimeStop")
        antiTimeStopBtn.Font = Enum.Font.GothamBold
        antiTimeStopBtn.TextSize = 14
