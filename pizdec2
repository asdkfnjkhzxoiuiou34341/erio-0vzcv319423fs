    configTextBox.Text = ""
end)

local selectedConfigName = ""
local function refreshConfigDropdown()
    local configNames = getConfigNames(true)
    if #configNames > 0 then
        selectedConfigName = configNames[1] -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–µ—Ä–≤—ã–π –∫–æ–Ω—Ñ–∏–≥ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    end
    return configNames
end

