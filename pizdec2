-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏
local StorageSection = CreateSection(ConfigTab, "üíæ –°–∏—Å—Ç–µ–º–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è")

CreateButton(StorageSection, "–°—Ç–∞—Ç—É—Å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è", "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –º–µ—Ç–æ–¥–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è", function()
    local message = "–≠–∫—Å–ø–ª–æ–π—Ç: " .. executorName .. "\n"
    message = message .. "–ú–µ—Ç–æ–¥ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è: " .. storageMethod .. "\n"
    message = message .. "–î–µ—Ç–∞–ª–∏: " .. storageInfo
    
    if storageMethod == "files" then
        message = message .. "\n‚úÖ –§–∞–π–ª—ã —Å–æ—Ö—Ä–∞–Ω—è—é—Ç—Å—è –Ω–∞–≤—Å–µ–≥–¥–∞"
        message = message .. "\n‚úÖ –ê–≤—Ç–æ–∑–∞–≥—Ä—É–∑–∫–∞ –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ"
    elseif storageMethod == "global" then
        message = message .. "\n‚ö†Ô∏è –ö–æ–Ω—Ñ–∏–≥–∏ –æ—Å—Ç–∞—é—Ç—Å—è –ø–æ–∫–∞ –æ—Ç–∫—Ä—ã—Ç —ç–∫—Å–ø–ª–æ–π—Ç"
        message = message .. "\n‚úÖ –ê–≤—Ç–æ–∑–∞–≥—Ä—É–∑–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–µ —Å–∫—Ä–∏–ø—Ç–∞"
    elseif storageMethod == "clipboard" then
        message = message .. "\n‚ö†Ô∏è –ù—É–∂–Ω–æ –≤—Ä—É—á–Ω—É—é —Å–æ—Ö—Ä–∞–Ω—è—Ç—å –≤ –±–ª–æ–∫–Ω–æ—Ç"
        message = message .. "\nüìã –ö–æ–Ω—Ñ–∏–≥–∏ –∫–æ–ø–∏—Ä—É—é—Ç—Å—è –≤ –±—É—Ñ–µ—Ä –æ–±–º–µ–Ω–∞"
    else
        message = message .. "\n‚ùå –ö–æ–Ω—Ñ–∏–≥–∏ —Ç–æ–ª—å–∫–æ –≤ –ø–∞–º—è—Ç–∏"
        message = message .. "\n‚ùå –ò—Å—á–µ–∑–∞—é—Ç –ø—Ä–∏ –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–µ"
    end
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "–°–∏—Å—Ç–µ–º–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è",
        Text = message,
        Duration = 8
    })
end)

CreateButton(StorageSection, "–ü–æ—Å–º–æ—Ç—Ä–µ—Ç—å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–µ –∫–æ–Ω—Ñ–∏–≥–∏", "–°–ø–∏—Å–æ–∫ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã—Ö –∫–æ–Ω—Ñ–∏–≥–æ–≤", function()
    if storageMethod == "none" then
        game.StarterGui:SetCore("SendNotification", {
            Title = "SSLKin Uni Script",
            Text = "–ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Ö—Ä–∞–Ω–∏–ª–∏—â–µ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω–æ!",
            Duration = 3
        })
        return
    end
    
    spawn(function()
        local savedConfigs = getPersistentConfigNames()
        local message = "–°–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–µ –∫–æ–Ω—Ñ–∏–≥–∏: " .. #savedConfigs
        if #savedConfigs > 0 then
            message = message .. "\n" .. table.concat(savedConfigs, ", ")
        else
            message = message .. "\n–•—Ä–∞–Ω–∏–ª–∏—â–µ –ø—É—Å—Ç–æ–µ"
        end
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "–°–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–µ –∫–æ–Ω—Ñ–∏–≥–∏",
            Text = message,
            Duration = 6
        })
    end)
end)

-- –í–ö–õ–ê–î–ö–ê –†–ê–ó–ù–û–ï
local UtilitySection = CreateSection(MiscTab, "üîß –£—Ç–∏–ª–∏—Ç—ã")

CreateButton(UtilitySection, "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –∏–≥—Ä–µ", "–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç –¥–∞–Ω–Ω—ã–µ –æ —Ç–µ–∫—É—â–µ–π –∏–≥—Ä–µ", function()
    local success, gameInfo = pcall(function()
        return game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
    end)
    
    if success then
        local info = string.format(
            "–ò–≥—Ä–∞: %s\n–†–µ–∂–∏–º: %s\n–ò–≥—Ä–æ–∫–æ–≤: %d\n–í–∞—à –ø–∏–Ω–≥: %d –º—Å\nPlace ID: %d",
            gameInfo.Name,
            currentGame,
            #Players:GetPlayers(),
            math.floor(LocalPlayer:GetNetworkPing() * 1000),
            game.PlaceId
        )
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –∏–≥—Ä–µ",
            Text = info,
            Duration = 10
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "–û—à–∏–±–∫–∞",
            Text = "–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ–± –∏–≥—Ä–µ",
            Duration = 3
        })
    end
end)

-- –°–∏—Å—Ç–µ–º–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
local function setupInfiniteJump()
    UserInputService.JumpRequest:Connect(function()
        if infiniteJumpEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
end

local function setupNoclip()
    if currentGame ~= "Centaura" then
        RunService.Stepped:Connect(function()
            if isNoclipActive and LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
            elseif not isNoclipActive and LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                        part.CanCollide = true
                    end
                end
            end
        end)
    end
end

-- Aimbot –¥–ª—è Centaura (—É–ª—É—á—à–µ–Ω–Ω—ã–π —Å –ø—Ä–µ–¥–∏–∫—Ç–æ–º)
if currentGame == "Centaura" then
    local lastTargetCheck = 0
    local currentTarget = nil
    local targetCheckInterval = 0.08 -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ü–µ–ª–∏ –∫–∞–∂–¥—ã–µ 80–º—Å –¥–ª—è –ª—É—á—à–µ–π –æ—Ç–∑—ã–≤—á–∏–≤–æ—Å—Ç–∏
    local playerVelocities = {} -- –û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏–≥—Ä–æ–∫–æ–≤ –¥–ª—è –ø—Ä–µ–¥–∏–∫—Ç–∞
    
    local function getClosestEnemy()
        local closestPlayer = nil
        local shortestDistance = math.huge
        
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ —É –∏–≥—Ä–æ–∫–∞ –µ—Å—Ç—å –Ω—É–∂–Ω–∞—è —á–∞—Å—Ç—å —Ç–µ–ª–∞
                local targetPart = nil
                if aimbotTargetPart == "Head" then
                    targetPart = player.Character:FindFirstChild("Head")
                elseif aimbotTargetPart == "Torso" then
                    targetPart = player.Character:FindFirstChild("Torso") or player.Character:FindFirstChild("UpperTorso")
                elseif aimbotTargetPart == "Left Arm" then
                    targetPart = player.Character:FindFirstChild("Left Arm") or player.Character:FindFirstChild("LeftUpperArm")
                elseif aimbotTargetPart == "Right Arm" then
                    targetPart = player.Character:FindFirstChild("Right Arm") or player.Character:FindFirstChild("RightUpperArm")
                elseif aimbotTargetPart == "Left Leg" then
                    targetPart = player.Character:FindFirstChild("Left Leg") or player.Character:FindFirstChild("LeftUpperLeg")
                elseif aimbotTargetPart == "Right Leg" then
                    targetPart = player.Character:FindFirstChild("Right Leg") or player.Character:FindFirstChild("RightUpperLeg")
                end
                
                if not targetPart then
                    continue -- –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –∏–≥—Ä–æ–∫–∞ –µ—Å–ª–∏ –Ω–µ—Ç –Ω—É–∂–Ω–æ–π —á–∞—Å—Ç–∏ —Ç–µ–ª–∞
                end
                
                if aimbotTeamCheck and not isPlayerEnemy(player) then
                    continue
                end
                
                local vector, onScreen = Camera:WorldToViewportPoint(targetPart.Position)
                
                if onScreen then
                    local distance = (Vector2.new(vector.X, vector.Y) - Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)).Magnitude
                    
                    if distance < aimbotFOV and distance < shortestDistance then
                        if aimbotVisibleOnly then
                            -- –£–ª—É—á—à–µ–Ω–Ω—ã–π raycast —Å –±–æ–ª—å—à–µ–π —Ç–æ—á–Ω–æ—Å—Ç—å—é
                            local raycastParams = RaycastParams.new()
                            raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
                            raycastParams.FilterDescendantsInstances = {LocalPlayer.Character, player.Character}
                            
                            local direction = (targetPart.Position - Camera.CFrame.Position).Unit
                            local raycastResult = Workspace:Raycast(Camera.CFrame.Position, direction * 1000, raycastParams)
                            
                            if not raycastResult then
                                closestPlayer = player
                                shortestDistance = distance
                            end
                        else
                            closestPlayer = player
                            shortestDistance = distance
                        end
                    end
                end
            end
        end
        
        return closestPlayer
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–µ–¥–∏–∫—Ç–∞ –¥–≤–∏–∂–µ–Ω–∏—è
    local function predictTargetPosition(player, targetPart)
        if not aimbotPrediction or not playerVelocities[player] or not playerVelocities[player].velocity then
            return targetPart.Position
        end
        
        local velocity = playerVelocities[player].velocity
        local distance = (Camera.CFrame.Position - targetPart.Position).Magnitude
        local bulletTime = distance / 400 -- –ü—Ä–∏–º–µ—Ä–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å –ø—É–ª–∏ –≤ Centaura
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ —Å–∫–æ—Ä–æ—Å—Ç—å –Ω–µ —Å–ª–∏—à–∫–æ–º –±–æ–ª—å—à–∞—è (–∏–∑–±–µ–≥–∞–µ–º –≥–ª—é–∫–æ–≤)
        if velocity.Magnitude > 100 then
            return targetPart.Position -- –ï—Å–ª–∏ —Å–∫–æ—Ä–æ—Å—Ç—å —Å–ª–∏—à–∫–æ–º –±–æ–ª—å—à–∞—è, –Ω–µ –∏—Å–ø–æ–ª—å–∑—É–µ–º –ø—Ä–µ–¥–∏–∫—Ç
        end
        
        -- –ü—Ä–µ–¥–∏–∫—Ç–∏–º –ø–æ–∑–∏—Ü–∏—é —Å —É—á–µ—Ç–æ–º —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ —Å–∏–ª—ã –ø—Ä–µ–¥–∏–∫—Ç–∞
        local predictedPosition = targetPart.Position + (velocity * bulletTime * aimbotPredictionStrength)
        return predictedPosition
    end
    
    -- –û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏–≥—Ä–æ–∫–æ–≤
    spawn(function()
        while true do
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local currentPos = player.Character.HumanoidRootPart.Position
                    if playerVelocities[player] and playerVelocities[player].lastPos then
                        local deltaTime = 0.1
                        local velocity = (currentPos - playerVelocities[player].lastPos) / deltaTime
                        playerVelocities[player] = {
                            velocity = velocity,
                            lastPos = currentPos
                        }
                    else
                        playerVelocities[player] = {
                            velocity = Vector3.new(0, 0, 0),
                            lastPos = currentPos
                        }
                    end
                else
                    playerVelocities[player] = nil
                end
            end
            wait(0.1)
        end
    end)
    
    -- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ –∞–∏–º–±–æ—Ç–∞
    RunService.Heartbeat:Connect(function()
        if not aimbotEnabled then
            currentTarget = nil
            return
        end
        
        local currentTime = tick()
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º —Ü–µ–ª—å —Ç–æ–ª—å–∫–æ –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏
        if currentTime - lastTargetCheck >= targetCheckInterval then
            currentTarget = getClosestEnemy()
            lastTargetCheck = currentTime
        end
        
        -- –ü—Ä–∏–º–µ–Ω—è–µ–º –ø—Ä–∏—Ü–µ–ª–∏–≤–∞–Ω–∏–µ –∫ —Ç–µ–∫—É—â–µ–π —Ü–µ–ª–∏
        if currentTarget and currentTarget.Character then
            local targetPart = nil
            if aimbotTargetPart == "Head" then
                targetPart = currentTarget.Character:FindFirstChild("Head")
            elseif aimbotTargetPart == "Torso" then
                targetPart = currentTarget.Character:FindFirstChild("Torso") or currentTarget.Character:FindFirstChild("UpperTorso")
            elseif aimbotTargetPart == "Left Arm" then
                targetPart = currentTarget.Character:FindFirstChild("Left Arm") or currentTarget.Character:FindFirstChild("LeftUpperArm")
            elseif aimbotTargetPart == "Right Arm" then
                targetPart = currentTarget.Character:FindFirstChild("Right Arm") or currentTarget.Character:FindFirstChild("RightUpperArm")
            elseif aimbotTargetPart == "Left Leg" then
                targetPart = currentTarget.Character:FindFirstChild("Left Leg") or currentTarget.Character:FindFirstChild("LeftUpperLeg")
            elseif aimbotTargetPart == "Right Leg" then
                targetPart = currentTarget.Character:FindFirstChild("Right Leg") or currentTarget.Character:FindFirstChild("RightUpperLeg")
            end
            
            if targetPart then
                local targetPosition = predictTargetPosition(currentTarget, targetPart)
                local camera = Camera
                local currentCFrame = camera.CFrame
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ —Ü–µ–ª—å –≤ —Ä–∞–∑—É–º–Ω—ã—Ö –ø—Ä–µ–¥–µ–ª–∞—Ö
                local distance = (currentCFrame.Position - targetPosition).Magnitude
                if distance > 5 and distance < 1500 then
                    local targetCFrame = CFrame.lookAt(currentCFrame.Position, targetPosition)
                    
                    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —É–≥–æ–ª –ø–æ–≤–æ—Ä–æ—Ç–∞ —á—Ç–æ–±—ã –∏–∑–±–µ–∂–∞—Ç—å —Ä–µ–∑–∫–∏—Ö –¥–≤–∏–∂–µ–Ω–∏–π
                    local angle = math.acos(math.clamp(currentCFrame.LookVector:Dot(targetCFrame.LookVector), -1, 1))
                    
                    if angle < math.rad(120) then -- –£–≤–µ–ª–∏—á–∏–ª–∏ –¥–æ 120 –≥—Ä–∞–¥—É—Å–æ–≤ –¥–ª—è –ª—É—á—à–µ–≥–æ –∑–∞—Ö–≤–∞—Ç–∞
                        local lerpFactor = math.min(1 / aimbotSmoothness, 0.3) -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–ª–∏ –º–∞–∫—Å–∏–º–∞–ª—å–Ω—É—é —Å–∫–æ—Ä–æ—Å—Ç—å
                        local newCFrame = currentCFrame:Lerp(targetCFrame, lerpFactor)
                        
                        -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –≤–∞–ª–∏–¥–Ω–æ—Å—Ç–∏
                        if newCFrame.Position.Y > currentCFrame.Position.Y - 100 then
                            camera.CFrame = newCFrame
                        end
                    else
                        currentTarget = nil -- –¶–µ–ª—å —Å–ª–∏—à–∫–æ–º –¥–∞–ª–µ–∫–æ –ø–æ —É–≥–ª—É
                    end
                else
                    currentTarget = nil -- –¶–µ–ª—å —Å–ª–∏—à–∫–æ–º –¥–∞–ª–µ–∫–æ –∏–ª–∏ –±–ª–∏–∑–∫–æ
                end
            else
                currentTarget = nil -- –ß–∞—Å—Ç—å —Ç–µ–ª–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞
            end
        end
    end)
    
    -- –û—á–∏—Å—Ç–∫–∞ —Å–∫–æ—Ä–æ—Å—Ç–µ–π –ø—Ä–∏ –≤—ã—Ö–æ–¥–µ –∏–≥—Ä–æ–∫–æ–≤
    Players.PlayerRemoving:Connect(function(player)
        playerVelocities[player] = nil
    end)
end

-- –£–ª—É—á—à–µ–Ω–Ω–æ–µ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ —á–∞–º—Å–æ–≤
spawn(function()
    while true do
        if espSettings.enabled and espSettings.chams then
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer then
                    updatePlayerChams(player)
                end
            end
        else
            -- –ï—Å–ª–∏ —á–∞–º—Å—ã –≤—ã–∫–ª—é—á–µ–Ω—ã, –æ—á–∏—â–∞–µ–º –≤—Å–µ
            for player, cham in pairs(playerChams) do
                if cham and cham.Parent then
                    pcall(function()
                        cham:Destroy()
                    end)
                end
            end
            playerChams = {}
        end
        wait(0.5) -- –û–±–Ω–æ–≤–ª—è–µ–º —á–∞–º—Å—ã –∫–∞–∂–¥—ã–µ 0.5 —Å–µ–∫—É–Ω–¥—ã –¥–ª—è –ª—É—á—à–µ–π –æ—Ç–∑—ã–≤—á–∏–≤–æ—Å—Ç–∏
    end
end)

-- –≠—Ç–∏ –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —É–∂–µ –µ—Å—Ç—å –≤—ã—à–µ, —É–±–∏—Ä–∞–µ–º –¥—É–±–ª–∏–∫–∞—Ç—ã

-- –§—É–Ω–∫—Ü–∏—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –Ω–∞—Å—Ç—Ä–æ–µ–∫ –ø–æ—Å–ª–µ —Ä–µ—Å–ø–∞–≤–Ω–∞
local function restorePlayerSettings()
    if LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            -- –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏
            if speedEnabled then
                humanoid.WalkSpeed = walkSpeed
            end
            
            -- –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø—Ä—ã–∂–∫–∞
            if jumpEnabled then
                humanoid.JumpHeight = jumpPower
            end
        end
    end
end

-- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ —Ä–µ—Å–ø–∞–≤–Ω–∞ –ª–æ–∫–∞–ª—å–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞
LocalPlayer.CharacterAdded:Connect(function(character)
    wait(1) -- –ñ–¥–µ–º –ø–æ–ª–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    restorePlayerSettings()
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º ESP
    if espSettings.enabled then
        wait(0.5)
        updateESP()
    end
end)

-- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ —Å–º–µ—Ä—Ç–∏ –∏–≥—Ä–æ–∫–æ–≤ (—É–ø—Ä–æ—â–µ–Ω–Ω—ã–π)
local connectedPlayers = {}

local function connectPlayerDeath(player)
    if connectedPlayers[player] or not player.Character then return end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if humanoid then
        local connection = humanoid.Died:Connect(function()
            -- –£–¥–∞–ª—è–µ–º —á–∞–º—Å —Å—Ä–∞–∑—É –ø—Ä–∏ —Å–º–µ—Ä—Ç–∏
            if playerChams[player] then
                pcall(function()
                    playerChams[player]:Destroy()
                end)
                playerChams[player] = nil
            end
            
            wait(0.5)
            if espSettings.enabled then
                updateESP()
            end
            connectedPlayers[player] = nil -- –£–±–∏—Ä–∞–µ–º –∏–∑ —Å–ø–∏—Å–∫–∞ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏
        end)
        connectedPlayers[player] = connection
    end
end

-- –ü–æ–¥–∫–ª—é—á–∞–µ–º –¥–ª—è –≤—Å–µ—Ö —Ç–µ–∫—É—â–∏—Ö –∏–≥—Ä–æ–∫–æ–≤
for _, player in pairs(Players:GetPlayers()) do
    connectPlayerDeath(player)
end

-- –ü–æ–¥–∫–ª—é—á–∞–µ–º –¥–ª—è –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        wait(1)
        connectPlayerDeath(player)
        if espSettings.enabled then
            updateESP()
        end
    end)
end)

-- –û—á–∏—â–∞–µ–º —Å–æ–µ–¥–∏–Ω–µ–Ω–∏—è –ø—Ä–∏ –≤—ã—Ö–æ–¥–µ –∏–≥—Ä–æ–∫–∞
Players.PlayerRemoving:Connect(function(player)
    if connectedPlayers[player] then
        connectedPlayers[player]:Disconnect()
        connectedPlayers[player] = nil
    end
    
    -- –£–¥–∞–ª—è–µ–º —á–∞–º—Å –∏–≥—Ä–æ–∫–∞ –∫–æ—Ç–æ—Ä—ã–π –ø–æ–∫–∏–Ω—É–ª
    if playerChams[player] then
        pcall(function()
            playerChams[player]:Destroy()
        end)
        playerChams[player] = nil
    end
    
    if espSettings.enabled then
        updateESP()
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–Ω–æ–ø–æ–∫
CloseButton.MouseButton1Click:Connect(HideGUI)

-- –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏
spawn(function()
    wait(1) -- –ñ–¥–µ–º –ø–æ–ª–Ω–æ–π –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        if input.KeyCode == Enum.KeyCode.Insert then
            print("SSLKin: Insert –Ω–∞–∂–∞—Ç, MainFrame.Visible =", MainFrame.Visible) -- –û—Ç–ª–∞–¥–∫–∞
            if MainFrame.Visible then
                HideGUI()
            else
                ShowGUI()
            end
        end
    end)
    print("SSLKin: –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞–Ω—ã")
end)

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
setupInfiniteJump()
setupNoclip()

-- –ê–∫—Ç–∏–≤–∞—Ü–∏—è –ø–µ—Ä–≤–æ–π –≤–∫–ª–∞–¥–∫–∏
if #tabs > 0 then
    tabs[1].button.MouseButton1Click()
end

-- –ü–æ–∫–∞–∑–∞—Ç—å GUI –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
ShowGUI()



-- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –æ –∑–∞–≥—Ä—É–∑–∫–µ
game.StarterGui:SetCore("SendNotification", {
    Title = "SSLKin Uni Script",
    Text = string.format("%s Mode v3.1 –∑–∞–≥—Ä—É–∂–µ–Ω! Insert –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è/–∑–∞–∫—Ä—ã—Ç–∏—è", currentGame),
    Duration = 5
})

print(string.format("SSLKin Uni Script v3.1 –∑–∞–≥—Ä—É–∂–µ–Ω –≤ —Ä–µ–∂–∏–º–µ %s!", currentGame))
print("–°–æ–∑–¥–∞–Ω–æ by SSLKin | –ù–∞–∂–º–∏—Ç–µ Insert –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è/–∑–∞–∫—Ä—ã—Ç–∏—è")
